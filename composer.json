{
    "name": "circulon/yii2-flag-behavior",
    "description": "Bitwise flag setting for Yii 2.x",
    "keywords": [
        "circulon",
        "yii",
        "yii2",
        "yii 2",
        "behavior",
        "flag",
        "bitwise"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/circulon/yii2-flag-behavior.git",
    "authors": [
        {
            "name": "Kieren Eaton",
            "email": "circledev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "circulon\\flag\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
